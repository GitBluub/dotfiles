[user]
	email = louis.auzuret@gmail.com
	name = GitBluub
	signingkey = /Users/lauzuret/.ssh/id_ed25519.pub
[pull]
	rebase = true
  ff = true
[push]
  autoSetupRemote = true
  default = "upstream"
[protocol "file"]
	allow = always
[gpg]
	format = ssh
[advice]
    diverging = false
[rebase]
    autoSquash = true
    autoStash = true
    updateRefs = true

[rerere]
    enabled = true
[branch]
    sort = "-committerdate"
